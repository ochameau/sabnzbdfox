<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet href="chrome://global/skin/" type="text/css"?><!DOCTYPE prefwindow SYSTEM "chrome://sabnzbdfox/locale/prefwindow.dtd"><prefwindow id="webfragPreferences" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul" title="&prefwindow.title;">  <script>    function selectTarget() {      var wm = Components.classes["@mozilla.org/appshell/window-mediator;1"]                   .getService(Components.interfaces.nsIWindowMediator);      var browserWindow = wm.getMostRecentWindow("navigator:browser");      browserWindow.sabnzbfox.selectTarget();    }    function onActionChange() {      var radio = document.getElementById("action-target");      var action = radio.value;      if (action=="api") {        document.getElementById("pref-action-api-block").hidden=false;        document.getElementById("pref-action-target-block").hidden=true;      } else {        document.getElementById("pref-action-api-block").hidden=true;        document.getElementById("pref-action-target-block").hidden=false;      }    }    function onPaneLoad() {      onActionChange();    }  </script>    <prefpane id="pane1" label="&pane1.title;" onpaneload="onPaneLoad()">    <preferences>      <preference id="pref-enabled" name="extensions.sabnzbfox.enabled" type="bool"/>      <preference id="pref-action" name="extensions.sabnzbfox.action" type="string" onchange="onActionChange()"/>      <preference id="pref-target" name="extensions.sabnzbfox.target" type="string"/>      <preference id="pref-host" name="extensions.sabnzbfox.host" type="string"/>      <preference id="pref-port" name="extensions.sabnzbfox.port" type="int"/>	  <preference id="pref-https" name="extensions.sabnzbfox.https" type="bool"/>      <preference id="pref-username" name="extensions.sabnzbfox.username" type="string"/>      <preference id="pref-password" name="extensions.sabnzbfox.password" type="string"/>      <preference id="pref-apikey" name="extensions.sabnzbfox.apikey" type="string"/>    </preferences>    <checkbox preference="pref-enabled" label="&enabledPref.label;" />        <groupbox>      <caption label="&action.label;"/>      <radiogroup id="action-target" preference="pref-action">        <radio label="&targetPref.label;" id="pref-action-target" value="target" />        <hbox class="indent" align="center" id="pref-action-target-block">          <button label="&targetPref.select;" oncommand="selectTarget()" />          <textbox id="textbox1" flex="1" preference="pref-target"/>        </hbox>        <radio label="&apiPref.label;" id="pref-action-api" value="api"/>        <hbox class="indent" id="pref-action-api-block">          <label style="font-weight: bold;" value="&apiPref.notice;" />        </hbox>      </radiogroup>    </groupbox>    <groupbox>      <caption label="&serverInfo.label;"/>      <hbox align="center">        <label style="width: 70px; text-align: right;" control="textbox2">&hostPref.label;</label>        <textbox id="textbox2" flex="1" preference="pref-host"/>      </hbox>      <hbox align="center">        <label style="width: 70px; text-align: right;" control="textbox3">&portPref.label;</label>        <textbox id="textbox3" flex="1" preference="pref-port"/>      </hbox>	  <hbox align="center">        <label style="width: 70px; text-align: right;" control="textbox3">&portHttps.label;</label>        <checkbox preference="pref-https" />      </hbox>      <hbox align="center">        <label style="width: 70px; text-align: right;" control="textbox3">&usernamePref.label;</label>        <textbox id="textbox3" flex="1" preference="pref-username"/>      </hbox>      <hbox align="center">        <label style="width: 70px; text-align: right;" control="textbox3">&passwordPref.label;</label>        <textbox id="textbox3" flex="1" type="password" preference="pref-password"/>      </hbox>      <hbox align="center">        <label style="width: 70px; text-align: right;" control="textbox4">&apiKeyPref.label;</label>        <textbox id="textbox4" flex="1" preference="pref-apikey"/>      </hbox>      <label>&apiKeyPref.explain;</label>    </groupbox>  </prefpane></prefwindow>